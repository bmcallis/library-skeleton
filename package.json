{
  "name": "library",
  "version": "0.1.0",
  "description": "Library Skeleton",
  "main": "lib/library.js",
  "scripts": {
    "clean": "rimraf lib/*",
    "build": "npm-run-all clean build:*",
    "build:js": "webpack --progress",
    "build:min": "npm run build:js -- --mode=min",
    "test": "mocha --compilers js:babel-core/register --colors --recursive ./test",
    "dev": "npm-run-all build:js --parallel watch:*",
    "watch:js": "npm run build:js -- --watch",
    "watch:test": "npm run test -- --watch",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint src",
    "lint:test": "eslint test",
    "verify": "npm-run-all lint test"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.5.0",
    "chai": "3.5.0",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "mocha": "2.4.5",
    "npm-run-all": "1.5.1",
    "rimraf": "2.5.2",
    "webpack": "1.12.14",
    "yargs": "4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bmcallis/library.git"
  },
  "keywords": [
    "npm scrips",
    "wepback",
    "babel",
    "mocha",
    "es6"
  ],
  "author": "Brandon McAllister <brandon@insomniacisland.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/bmcallis/library/issues"
  },
  "homepage": "https://gitlab.com/bmcallis/library"
}
